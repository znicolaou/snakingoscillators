import os.path

for i in range(41,0,-1):
  if(os.path.exists('chimeras/'+str(i)+'cycle.dat')):
    print("chimera"+str(i)+"\n\n")
    forward=run('janus_rel',c='forward',dat='chimeras/'+str(i)+'cycle.dat')
    forward=rl(forward)
    sv('janus_rel')
    reverse=run('janus_rel',c='reverse',dat='chimeras/'+str(i)+'cycle.dat')
    branch=mb(forward+reverse)
    branch=rl(branch)
    sv(branch,str(i))
    branch.writeRawFilename(str(i)+'.dat')
    dl('janus_rel')
cl()
